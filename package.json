{
  "name": "iframe-shell",
  "version": "0.0.4",
  "description": "mip iframe loader",
  "main": "src/index.js",
  "browser": "dist/iframe-shell.js",
  "scripts": {
    "test": "./node_modules/.bin/karma start --reporters mocha",
    "test-coverage": "./node_modules/.bin/karma start --reporters mocha,coverage",
    "test-html": "./node_modules/.bin/karma start --reporters mocha,html",
    "test-reports": "./node_modules/.bin/karma start --reporters mocha,coverage,html",
    "test-watch": "./node_modules/.bin/karma start --auto-watch --no-single-run",
    "test-listen": "./node_modules/.bin/karma start --browsers --no-single-run",
    "test-run": "./node_modules/.bin/karma run",
    "release": "rm -rf dist/*; rm -rf build/*; ./node_modules/.bin/fis3 release prod -d build; ./node_modules/.bin/fis3 release -d build; cp -r build/packed/* dist/",
    "preversion": "npm run release",
    "dev": "./node_modules/.bin/fis3 release -w -d build",
    "server": "./node_modules/.bin/fis3 server start --root .",
    "server-stop": "./node_modules/.bin/fis3 server stop",
    "doc": "rm -rf build/doc; ./node_modules/.bin/jsdoc -c ./jsdoc.conf.json",
    "clean": "rm -rf dist/*; rm -rf build/*"
  },
  "author": "oott123",
  "license": "UNLICENSED",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "fis-optimizer-uglify-js": "^0.2.2",
    "fis-parser-babel-5.x": "^1.4.0",
    "fis3": "^3.4.24",
    "fis3-hook-amd": "^0.2.0",
    "fis3-hook-commonjs": "^0.1.25",
    "fis3-hook-node_modules": "^2.2.8",
    "fis3-postpackager-loader": "^2.1.3",
    "jsdoc": "^3.4.2",
    "karma": "^0.12.31",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-html-reporter": "^0.2.7",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "mocha": "^2.5.3",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "micro-event": "^1.1.0"
  }
}
